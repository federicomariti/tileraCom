offset = 1

c1 = "#ccccff"
c2 = "#9999cc"
c3 = "#666699"
c4 = "#333366"
c5 = "#000033"
set grid
set linetype 0+offset lw 3 ps 1.25 lc rgb "#ee82ee"
set linetype 1+offset lw 3 ps 1.25 lc rgb c1
set linetype 2+offset lw 3 ps 1.25 lc rgb c2
set linetype 3+offset lw 3 ps 1.25 lc rgb c3
set linetype 4+offset lw 3 ps 1.25 lc rgb c4
set linetype 5+offset lw 3 ps 1.25 lc rgb c5
